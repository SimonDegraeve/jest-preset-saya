{
  "globals": {
    "__DEV__": true
  },
  "clearMocks": true,
  "coverageDirectory": "node_modules/.coverage",
  "cacheDirectory": "node_modules/.cache",
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "src/cli.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
